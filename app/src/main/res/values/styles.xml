<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="tabbar_btn_pressed">
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@drawable/tabbar_btn_pressed</item>
    </style>

    <style name="tabbar_btn_normal">
        <item name="android:textColor">#3C7C77</item>
        <item name="android:background">@drawable/tabbar_btn_normal</item>
    </style>

<!--    Progress button-->
    <style name="progress_btn_check">
        <item name="android:background">@drawable/progress_btn_background</item>
        <item name="android:elevation">2dp</item>
    </style>
    <style name="progress_btn_notcheck">
        <item name="android:background">@null</item>
    </style>

</resources>